@require '../../main'

#app-header
  overflow hidden
  text-align: center
  h1
    display inline-block
  h1
    margin 0.3em 0.5em 0em 1.5em
  +above(mobile)
    h1
      margin-right 1em
  +above(tablet)
    h1
      margin 0.3em 1em 0.3em 4em   
    
  ul
    inline-list()
    display inline-block
    position: relative
    top 0.5em